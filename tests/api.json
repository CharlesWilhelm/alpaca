{
	"base": "http://localhost:3000",
	"version": "v1",
	"authorization": {
		"basic" : true,
		"header": true,
		"header_prefix": "l00mPa",
		"oauth" : true
	},
	"request": {
		"formats": {
			"form": true,
			"json": true
		}
	},
	"response": {
		"formats": {
			"default": "json",
			"html": true,
			"json": true
		},
		"suffix": true
	},
	"error": {
		"message": "error"
	},
	"class": {
		"test": {
			"equal": {
				"path": "/test/equal"
			},
			"end": {
				"path": "/test/end"
			}
		},
		"client_options": {
			"basic": {
				"path": "/index"
			}
		},
		"request_options": {
			"basic": {
				"path": "/index"
			},
			"suffix": {
				"path": "/suffix"
			}
		},
		"get": {
			"api": {
				"path": "/get/api",
				"params": [
					{
						"name": "first",
						"required": true
					},
					{
						"name": "second",
						"required": true
					},
					{
						"name": "optional"
					}
				]
			},
			"options": {
				"path": "/get/options"
			}
		},
		"post": {
			"empty_raw": {
				"path": "/post/empty_raw",
				"method": "post"
			},
			"options_raw": {
				"path": "/post/options_raw",
				"method": "post"
			},
			"empty_form": {
				"path": "/post/empty_form",
				"method": "post"
			},
			"api_form": {
				"path": "/post/api_form",
				"method": "post",
				"params": [
					{
						"name": "first",
						"required": true
					},
					{
						"name": "second",
						"required": true
					}
				]
			},
			"options_form": {
				"path": "/post/options_form",
				"method": "post"
			},
			"array_form": {
				"path": "/post/array_form",
				"method": "post"
			},
			"object_form": {
				"path": "/post/object_form",
				"method": "post"
			},
			"empty_json": {
				"path": "/post/empty_json",
				"method": "post"
			},
			"api_json": {
				"path": "/post/api_json",
				"method": "post",
				"params": [
					{
						"name": "first",
						"required": true
					},
					{
						"name": "second",
						"required": true
					}
				]
			},
			"options_json": {
				"path": "/post/options_json",
				"method": "post"
			}
		},
		"methods": {
			"patch": {
				"path": "/methods/patch",
				"method": "patch"
			},
			"put": {
				"path": "/methods/put",
				"method": "put"
			},
			"delete": {
				"path": "/methods/delete",
				"method": "delete"
			}
		},
		"response": {
			"basic": {
				"path": "/response/basic"
			},
			"header": {
				"path": "/response/header"
			},
			"html": {
				"path": "/response/html"
			},
			"json": {
				"path": "/response/json"
			}
		},
		"paths": {
			"args": ["foo"],
			"basic": {
				"path": "/paths/:foo/bar"
			},
			"no_arg": {
				"path": "/paths/:id/:foo/7/rank"
			}
		},
		"auth": {
			"basic": {
				"path": "/auth/basic"
			},
			"header": {
				"path": "/auth/header"
			},
			"oauth_secret": {
				"path": "/auth/oauth_secret"
			},
			"oauth_token": {
				"path": "/auth/oauth_token"
			}
		}
	}
}
